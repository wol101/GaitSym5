<?xml version="1.0" encoding="utf-8"?>
<preset name="linux-gcc-cpu-only-custom" comment="Linux gcc PhysX SDK general settings (cpu only) customised wis">
  <platform targetPlatform="linux" compiler="gcc" />
  <CMakeSwitches>
    <cmakeSwitch name="PX_BUILDSNIPPETS" value="False" comment="Generate the snippets" />
    <cmakeSwitch name="PX_BUILDPVDRUNTIME" value="False" comment="Generate the OmniPVD project" />
    <cmakeSwitch name="PX_FLOAT_POINT_PRECISE_MATH" value="True" comment="Generate static libs" />    
    <cmakeSwitch name="PX_GENERATE_STATIC_LIBRARIES" value="True" comment="Generate static libs" />    
  </CMakeSwitches>
  <CMakeParams>
    <cmakeParam name="CMAKE_INSTALL_PREFIX" value="install/linux-gcc-cpu-only-custom/PhysX" comment="Install path relative to PhysX SDK root" />
  </CMakeParams>
</preset>